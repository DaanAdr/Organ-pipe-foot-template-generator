<UserControl x:Class="Organ_Pipe_Foot_Model_Generator.Views.ReadFromExcel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Organ_Pipe_Foot_Model_Generator.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Button x:Name="btnReadCsv" Content="Excel bestand inlezen" HorizontalAlignment="Left" Margin="10,87,0,0" VerticalAlignment="Top" Click="btnReadCsv_Click"/>
        <Button x:Name="btnSave" Content="Sla CAD bestand op" HorizontalAlignment="Left" Margin="143,87,0,0" VerticalAlignment="Top" IsEnabled="False" Width="117" Click="btnSave_Click"/>
        <Label x:Name="lblSeperatorXAxis" Content="Horizontale afstand tussen voetstukken:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txbSeperatorXAxis" HorizontalAlignment="Left" Margin="254,14,0,0" TextWrapping="Wrap" Text="10" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblSeperatorYAxis" Content="Verticale afstand tussen voetztukken:" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txbSeperatorYAxis" HorizontalAlignment="Left" Margin="254,45,0,0" TextWrapping="Wrap" Text="10" VerticalAlignment="Top" Width="120"/>

    </Grid>
</UserControl>
